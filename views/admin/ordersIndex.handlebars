<h1 style="text-align:center">Admin Orders Index</h1>

<div class="container">
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th>Date</th>
        <th>Revenue</th>
        <th>Email</th>
        <th>Stripe Token</th>
        <th>Customer State</th>
      </tr>
    </thead>
    {{#each orders as |order|}}
      
        <tr>
        
          <td><a href="admin/{{order.id}}">{{order.createdAt}}</a></td>
          <td>${{order.revenue}}</td>
          <td>{{order.email}}</td>
          <td>{{order.stripeToken}}</td>
          <td>{{order.state}}</td>
        </tr>
     
    {{/each}}
  </table>
</div>


